    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "actioncable": "^5.2.4-3",
    "bootstrap": "^4.3.1",
    "camelcase-keys": "^6.1.0",
    "chart.js": "^2.9.3",
    "mdbreact": "^4.24.0",
    "moment": "^2.27.0",
    "mui-datatables": "^3.3.1",
    "node-sass": "^4.13.1",
    "ramda": "^0.26.1",
    "react": "^16.10.2",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-calendar-heatmap": "^1.8.1",
    "react-chartjs-2": "^2.10.0",
    "react-color": "^2.18.1",
    "react-content-loader": "^5.1.0",
    "react-dom": "^16.10.2",
    "react-icons": "^3.7.0",
    "react-json-pretty": "^2.2.0",
    "react-markdown": "^4.2.2",
    "react-native-svg": "^12.1.0",
    "react-nestable": "^1.2.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-toastify": "^6.0.8",
    "react-toggle": "^4.1.1",
    "rxjs": "^6.5.3",
    "snakecase-keys": "^3.1.0",
    "styled-components": "latest"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o build-css/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o build-css/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
